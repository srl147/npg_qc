				{ snp_name => 'rs9522149', ref_allele => 'T', alt_allele => 'C', chr => 'chr13', pos => '111174820', allele_swap => 0 },
				{ snp_name => 'rs985492', ref_allele => 'G', alt_allele => 'A', chr => 'chr18', pos => '31731071', allele_swap => 0 },
			]
	},
}
%]

[%#
End of Sequenom assay data
